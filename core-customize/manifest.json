{
    "commerceSuiteVersion": "1905.0",
    "extensions": [],
    "useConfig": {
        "properties": [
            {
                "location": "hybris/config/local.properties"
            }
        ],
        "extensions": {
            "location": "hybris/config/localextensions.xml"
        }
    },

    "aspects": [
        {
            "name": "backoffice",
            "webapps": [
                {
                    "name": "backoffice",
                    "contextPath": "/backoffice"
                },
                {
                    "name": "zddsopstorefront",
                    "contextPath": ""
                }
            ]
        },
        {
            "name": "backgroundProcessing",
            "webapps": [
                {
                    "name": "hac",
                    "contextPath": "/hac"
                },
                {
                    "name": "zddsb2cstorefront",
                    "contextPath": ""
                }
            ]
        },
	{
            "name": "api",
            "webapps": [
                {
                    "name": "zddswebservices",
                    "contextPath": ""
                },
                {
                    "name": "oauth2",
                    "contextPath": "/authorizationserver"
                }
            ]
        },
        {
            "name": "accstorefront",
	    "properties": [
                {
                    "key": "spring.session.enabled",
                    "value": "true"
                },
                {
                    "key": "spring.session.zddsstorefront.save",
                    "value":"async"
                },
                {
                    "key": "spring.session.zddsstorefront.cookie.name",
                    "value": "JSESSIONID"
                },
                {
                    "key": "spring.session.zddsstorefront.cookie.path",
                    "value": "/"
                },
                {
                    "key":"storefrontContextRoot",
                    "value":""
                }
            ],
            "webapps": [
                {
                    "name": "zddsstorefront",
                    "contextPath": ""
                }
            ]
        }
    ]
}
